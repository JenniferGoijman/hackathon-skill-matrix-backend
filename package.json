    "cross-env": "^7.0.3",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "error-handler-module": "^1.0.5",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.6.2",
    "express-oas-validator": "^2.0.2",
    "fast-csv": "^4.3.6",
    "handy-postgres": "^1.3.1",
    "helmet": "^4.1.1",
    "hogan.js": "^3.0.2",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "make-manifest": "^1.0.4",
    "on-headers": "^1.0.2",
    "optimist": "^0.6.1",
    "optional": "^0.1.4",
    "passport": "^0.5.0",
    "passport-azure-ad": "^4.3.1",
    "pg": "^8.7.1",
